{
	"name": "Python 3",
	"build": {
		"dockerfile": "Dockerfile",
		"context": "..",
		"args": { 
			"VARIANT": "3.12",
			"NODE_VERSION": "lts/*"
		}
	},
	"settings": { 
		"python.defaultInterpreterPath": "/usr/local/bin/python",
		"python.linting.enabled": true
	},
	"extensions": [
		"ms-python.python",
		"ms-python.vscode-pylance",
		"humao.rest-client"
	],
	"remoteUser": "vscode",
	"postCreateCommand": "sh .devcontainer/startup.sh"
}
